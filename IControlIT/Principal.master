    <script type="text/javascript">

        // hack do Marcio para manter a sessão vida
        setInterval("keepSessionAlive()", 5 * 60 * 1000);
        function keepSessionAlive() {
            var img = new Image(1, 1);
            img.src = "/KeepSessionAlive.aspx";
        }

        //controle do modal
        var idModal;
        function Modal(idModal) { $(idModal).modal(); }
        function ModalClose(idModal) { $(idModal).modal("hide"); }

        //monta tamanho da tela
        window.onload = maxWindow;
        function maxWindow() {
            window.moveTo(0, 0);

            if (document.all) { top.window.resizeTo((screen.availWidth - 1), screen.availHeight); }

            else if (document.layers || document.getElementById) {
                if (top.window.outerHeight < screen.availHeight || top.window.outerWidth < screen.availWidth) {
                    top.window.outerHeight = screen.availHeight;
                    top.window.outerWidth = (screen.availWidth - 1);
                }
            }
        }

        function TabEstoque(pSelecao) {
            var pHidden = document.getElementById('ContentPlaceHolder1_hdfTab');
            pHidden.value = pSelecao;
        }

