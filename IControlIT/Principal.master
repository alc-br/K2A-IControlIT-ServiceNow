        <script type="text/javascript">

            var btnFerramenta, style, toggle;
            var isOpen = new Boolean(true);

            function TamanhoTela() {
                var larguraTela = screen.width;
                document.getElementById("Hdf_LarguraTela").value = larguraTela;

                // cria um elemento style para mudar o css
                style = document.createElement('style');
                isOpen = false;

                //btnFerramenta = document.getElementById('btFerramenta');
                //btnFerramenta.addEventListener("click", openDropCadastro, false);

            }
            window.onload = TamanhoTela;

            function openDropCadastro() {

                if (isOpen) {

                    document.getElementById("toggle").className = "dropdown-toggle";
                    style.innerHTML = '#divDropFerramentas { display: none; }';
                    document.head.appendChild(style);
                }
                else {

                    document.getElementById("toggle").className = "dropdown-toggle-open";
                    style.innerHTML = '#divDropFerramentas { display: block; }';
                    document.head.appendChild(style);
                }

                isOpen = !isOpen;
            }

            function openNav() {
                document.getElementById("mySidenav").style.width = "260px";
            }

            function closeNav() {
                document.getElementById("mySidenav").style.width = "0px";
            }

            //function closeSidebar() {
            //    $("#wrapper").toggleClass("toggled");
            //}

            $("#btnMenu").click(function (e) {
                e.preventDefault();
                $("#wrapper").toggleClass("toggled");
            });

            $("#btnCloseMenu").click(function (e) {
                e.preventDefault();
                $("#wrapper").toggleClass("toggled");
            });

            function disableScrolling() {
                var x = window.scrollX;
                var y = window.scrollY;
                window.onscroll = function () { window.scrollTo(x, y); };
            }

            function enableScrolling() {
                window.onscroll = function () { };
            }

        </script>

